{
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": ["eslint:recommended", "plugin:vue/essential"],
  "plugins": ["vue"],
  "rules": {
    "vue/multi-word-component-names": "off",
    "no-console": "off",
    "no-debugger": "off",
    "linebreak-style": [0, "error", "windows"]
  }
}
